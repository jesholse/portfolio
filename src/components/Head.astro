---
import { ViewTransitions } from "astro:transitions";
---
<head>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
        <link rel="stylesheet" href="/css/style.css">
		<meta name="viewport" content="width=device-width" />
		<meta name="generator" content={Astro.generator} />
		<title>Index</title>
        <script src="https://unpkg.com/htmx.org@1.9.5"></script>
        		<title>Astro</title>
		<script is:inline>
            document.addEventListener("astro:page-load", () => {
                htmx.process(document.body);
            });
		</script>
		 <link href='https://unpkg.com/css.gg@2.0.0/icons/css/comment.css' rel='stylesheet'>
		 <link href='https://unpkg.com/css.gg@2.0.0/icons/css/calendar-dates.css' rel='stylesheet'>
		 <link href='https://unpkg.com/css.gg@2.0.0/icons/css/user.css' rel='stylesheet'>
		<ViewTransitions></ViewTransitions>
</head>